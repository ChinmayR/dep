{
  "linters": {
    "go": {
      "type": "golint",
      "include": "(\\.go$)",
      "exclude": [
        "(^vendor/)"
      ]
    },
    "gofmt": {
      "type": "gofmt",
      "include": "(\\.go$)"
    },
    "govet": {
      "type": "govet",
      "include": "(\\.go$)",
      "exclude": [
        "(^vendor/)"
      ]
    }
  }
}